  "license": "MIT",
  "bugs": {
    "url": "https://github.com/foss-haas/react-joi/issues"
  },
  "homepage": "https://github.com/foss-haas/react-joi",
  "devDependencies": {
    "coveralls": "^2.11.2",
    "expect.js": "^0.3.1",
    "istanbul": "^0.3.2",
    "joi": "^4.9.0",
    "jshint": "^2.5.10",
    "mocha": "^2.0.1",
    "react": "^0.12.1"
  }
